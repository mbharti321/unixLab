[34m#DISPLAY ALL ARRAY ELEMENTS[m
function print_array [35m{[m[9;9H[38;5;130mecho[m[31m [m[38;5;130m"[m[35m${myArray[[m@[35m]}[m[38;5;130m"[10;9Hecho[m
