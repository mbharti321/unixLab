[36mmyArray[m=[35m([m[31m10[m [31m20[m [31m30[m forty fifty sixty [31m70[m [31m80[m [35m)[m

[34m#DISPLAY ALL ARRAY ELEMENTS[m
function print_array [35m{[m[9;9H[38;5;130mecho[m[31m [m[38;5;130m"[m[35m${myArray[[m@[35m]}[m[38;5;130m"[10;9Hecho[m
[35m}[m

[38;5;130mecho[m[31m -n [m[38;5;130m"[m[31mInitially Array elements : [m[35m`print_array`[m[38;5;130m"[m

[34m#2. DISPLAY ALL ELEMENTS IN A RANGE[m
[38;5;130mecho[m[31m [m[38;5;130m"[m[31mArray elements in range(2-5) : [m[35m${myArray[[m@[35m][m[97m[101m:[m[35m2[m[97m[101m:[m[35m5}[m[38;5;130m"[m
[34m#3. DISPLAY THE NUMBER OF ARRAY ELEMENTS[m
[38;5;130mecho[m[31m [m[38;5;130m"[m[31mNum. of array elements : [m[35m${[m[97m[101m#[m[35mmyArray[[m@[35m]}[m[38;5;130m"[m
[34m#4. REPLACE A SUBSTRING IN AN ARRAY ELEMENT[m
[38;5;130mecho[m[31m [m[38;5;130m"[m[31mAfter REPLACing f with F ARRAY ELEMENTs : [m[35m${myArray[[m@[35m][m[97m[101m//f/F[m[35m}[m[38;5;130m"[m

[34m#5. SORT AND DISPLAY THE ARRAY ELEMENT IN ASCENDING ORDER AND
#DESCENDING ORDER[m
[38;5;130mecho[m[31m -n [m[38;5;130m"[m[31mBefore sorting Array elements : [m[35m`print_array`[m[38;5;130m"[m
[36mmyArray[m=[35m([m$[35m([mfor l in [35m${myArray[[m@[35m]}[m[38;5;130m;[m do [38;5;130mecho[m[31m [m[35m$l[m[38;5;130m;[m done [38;5;130m|[m sort[35m))[m
[38;5;130mecho[m[31m -n [m[38;5;130m"[m[31mafter sorting Array elements : [m[35m`print_array`[m[38;5;130m"[m

[34m#6. COPY AN ARRAY ELEMENTS TO ANOTHER ARRAY

#7. DISPLAY THE SUM AND AVERAGE OF ODD POSITION ELEMENTS AND EVEN
#POSITION ELEMENTS OF AN ARRAY AND FIND WHICH POSITION NUMBERS
#ARE HAVING GREATER AVERAGE[m
